      </ul>
  </nav>
</div>

<div id="rodrigo_torres_portfolio" class="content_wrapper">

  <!-- DOCUMENT HEADER -->
  <header scroll ng-class="{small:changeClass, solidHeader:changeClass}">
      <a href="#rodrigo_torres_portfolio"><h1 ng-class="{smallH1:changeClass}">Rodrigo Torres</h1></a>
        <div class="local_weather"><span>Chicago 55&deg; f</span><span class="forecast">partly cloudy</span></div>
        <nav>
            <ul class="navigation">
              <li class="navlink linkActive"><a href="#contact_me">contact me</a></li>
              <li class="navlink"><a href="#my_work">Work</a></li>
              <li class="navlink"><a href="#about_me">About Me</a></li>
            </ul>
        </nav>
      <div class="main_hamIcon right">
            <div class="line1"></div>
            <div class="line2"></div>
            <div class="line3"></div>
      </div>
  </header>

  <!-- MAIN HERO CITYVIEW SECTION -->
  <section id="mainHero" ng-mouseover="letThemMove($event)">
    <article class="greetingView">
        <h1 class="greet_ms">
          <span class="greet"><span class="greet_anime"></span><span class="greet_int">hello</span></span><span class="city">Chicago</span>
        </h1>
        <div class="clouds">
            <div class="cloud_backGround">
                <img src="cloud.svg" alt="cloud illustration" />
                <img src="cloud.svg" alt="cloud illustration" />
                <img src="cloud.svg" alt="cloud illustration" />
                <img src="cloud.svg" alt="cloud illustration" />
            </div>
            <div class="cloud_foreFront">
                <img src="cloud.svg" alt="cloud illustration" />
                <img src="cloud.svg" alt="cloud illustration" />
                <img src="cloud.svg" alt="cloud illustration" />
                <img src="cloud.svg" alt="cloud illustration" />
            </div>
        </div>
        <img class="citySkyline" src="citybackground.svg" alt="Illustration of Chicago's city skyline." />
        <div class="colorOverlay"></div>
    </article>
  </section>

  <!-- AUDIO SECTION -->
  <section id="ct_audio">
    <article class="greetingView">
        <h2>start with tunes</h2>
        <!-- AUDIO PLAYER -->
        <div class="playerContainer unselectable ng-cloak" ng-mousemove="mouseMoving($event)" ng-mouseup="dragStop($event)">
          <audio id="audio" src="{{playList[0].song}}"></audio>
          <div class="imageDisplay">
              <div class="hamIcon" ng-click="showDetails()">
                  <div></div>
                  <div></div>
                  <div></div>
              </div>
              <div class="shadow"></div>
          </div>
          <div class="audioPlaylist" ng-show="showPlaylist">
            <div class="nowPlaying">
                <h2>Now Playing</h2>
                <p ng-bind="playList[songCount].description"></p>
                <p class="duration"><em ng-bind="currentTime | time">00:00</em> / <em ng-bind="totalTime | time">00:00</em></p>
                <div class="arrow" ng-click="hideDetails()"></div>
                <div class="listShadow"></div>
            </div>
            <div class="songList">
                <ol>
                  <li class="pickSong" ng-repeat="index in playList" ng-bind="index.description" ng-click="audioSelected($index,$event)"></li>
                </ol>
            </div>
          </div>
          <div class="controlsContainer" >
              <div class="topControls">
                  <div class="shuffle" ng-click="toggleShuffle()"></div>
                  <div id="progressMeter" >
                      <div id="progressBar"></div>
                      <div id="thumbScrubber" ng-mousedown="dragStart($event)"  style="left:{{scrubLeft}}px;"></div>
                      <div id="currentTime" ng-bind="currentTime | time">00:00</div>
                      <div id="totalTime" ng-bind="totalTime | time">00:00</div>
                  </div>
                  <div class="loop" ng-click="toggleLoop()"></div>
              </div>
              <div class="descriptionCtn">
                  <div class="songDescription" ng-repeat="i in getNumber(2) track by $index" ng-bind="songInfo"></div>
              </div>
              <div class="playPause">
                  <div class="prev" ng-click="restartSong()" ng-dblclick="prevSong()"></div>
                  <div class="play_pause" ng-click="togglePlay(0)"></div>
                  <div class="next" ng-click="nextSong()"></div>
              </div>
          </div>
      </div>
      <!-- END OF AUDIO PLAYER -->
      <p>
        Don't judge me for my music selection
      </p>
    </article>
  </section>

  <!-- MY WORK SECTION -->
  <section id="portfoliWork">
    <div class="cta_container">
        <div class="cta_btn small_btn left">contact me</div>
        <div class="switch right">
            <input type="radio" class="switch-input" name="view" value="code" id="code" checked>
            <label for="code" class="switch-label switch-label-off">Code</label>
            <input type="radio" class="switch-input" name="view" value="design" id="design">
            <label for="design" class="switch-label switch-label-on">Design</label>
            <span class="switch-selection"></span>
        </div>
    </div>

    <article class="myWork">
      <h2>Code</h2>

      <div class="card_ctn">

        <div class="single_card">
            <div class="card_image">
                <div class="cardOverlay"></div>
                <img src="defaultImage.jpg" alt="" />
            </div>
            <div class="card_meta">
                <h3>Email</h3>
            </div>
        </div>

        <div class="single_card">
            <div class="card_image">
                <img src="defaultImage.jpg" alt="" />
            </div>
            <div class="card_meta">
                <h3>Email</h3>
            </div>
        </div>

        <div class="single_card">
            <div class="card_image">
                <img src="defaultImage.jpg" alt="" />
            </div>
            <div class="card_meta">
                <h3>Email</h3>
            </div>
        </div>

        <div class="single_card">
            <div class="card_image">
                <img src="defaultImage.jpg" alt="" />
            </div>
            <div class="card_meta">
                <h3>Email</h3>
            </div>
        </div>

      </div>
    </article>

  </section>

  <!-- ABOUT ME SECTION -->
